-- --- 2. draw ---
-- pico-8 convention: tab 2 handles all rendering

function _draw()
    cls() -- clear the screen

    local mid_x = 64

    -- --- a. level selection display (top) ---
    print("level < "..level.." >", mid_x - 30, 4, 7) -- white text
    print("z to "..(performance_locked and "reset" or "lock in"), mid_x - 20, 12, 10) -- yellow text
    
    -- --- b. max stats display ---
    print("max perf: "..tostring(current_max_perf), 4, 20, 10) -- displays max points for current level
    print("max reward: "..tostring(current_max_reward), 4, 28, 10) -- displays max coins for current level


    -- --- c. main timing bar (horizontal - red) ---
    local h_bar_x = 10
    local h_bar_y = 40 -- shifted down to accommodate new stats
    local h_bar_w = 108
    local h_bar_h = 10
    local bar_color = 8 -- red/orange

    -- bar background frame
    rectfill(h_bar_x - 1, h_bar_y - 1, h_bar_x + h_bar_w + 1, h_bar_y + h_bar_h + 1, 1) -- dark blue frame

    -- bar foreground
    local current_w = (bar_value * h_bar_w)
    rectfill(h_bar_x, h_bar_y, h_bar_x + current_w, h_bar_y + h_bar_h, bar_color)
    
    -- add a visual marker for the locked ratio if one exists
    if performance_locked then
        local marker_x = h_bar_x + (target_ratio * h_bar_w)
        -- draw a white line where the player pressed z
        line(marker_x, h_bar_y - 2, marker_x, h_bar_y + h_bar_h + 2, 7) 
    end

    -- --- d. score bar growth (horizontal - green) ---
    local g_bar_x = 10
    local g_bar_y = 60 -- shifted down
    local g_bar_w = 108
    local g_bar_h = 8
    local green_color = 10 -- light green

    -- bar background frame
    rectfill(g_bar_x - 1, g_bar_y - 1, g_bar_x + g_bar_w + 1, g_bar_y + g_bar_h + 1, 1) -- dark blue frame

    -- bar foreground (draws based on actual_score / 100)
    local actual_w = (actual_score / 100.0) * g_bar_w
    rectfill(g_bar_x, g_bar_y, g_bar_x + actual_w, g_bar_y + g_bar_h, green_color)
    
    -- score label (shows points acquired)
    local score_str_display = ""
    local results_points = 0.0
    
    -- if locked, interpolate the displayed points based on the animation progress
    if performance_locked then
        local progress = 0.0
        if target_score > 0 then
            progress = actual_score / target_score
        else
            -- handle target_score = 0 (perfect miss), progress is 0
            progress = 0.0
        end
        
        -- interpolate the points value
        results_points = achieved_points * progress
        
        -- if animation is finished, show the exact final value
        if actual_score >= target_score and target_score > 0 then
            results_points = achieved_points
        elseif actual_score >= target_score and target_score == 0 then
            results_points = 0.0
        end
    end
    
    -- use 2 decimal places to prevent truncation of small values (< 0.1) to 0.0
    score_str_display = tostring(flr(results_points * 100) / 100) 
    if not performance_locked then score_str_display = "---" end

    -- display the calculated points. 
    print("points: "..score_str_display, mid_x - 10, g_bar_y + g_bar_h + 2, 10) -- green text


    -- --- e. max performance reference (vertical bar) ---
    local v_bar_x = 130
    local v_bar_y = 70 -- shifted down
    local v_bar_w = 10
    local v_bar_h = 60
    
    -- vertical bar frame (showing max potential)
    rectfill(v_bar_x - 1, v_bar_y - 1, v_bar_x + v_bar_w + 1, v_bar_y + v_bar_h + 1, 1)
    
    -- draw the max performance limit line based on the level data (max 100)
    local max_perf_ratio = current_max_perf / 100.0
    local max_perf_h = max_perf_ratio * v_bar_h
    line(v_bar_x - 1, v_bar_y + v_bar_h - max_perf_h, v_bar_x + v_bar_w + 1, v_bar_y + v_bar_h - max_perf_h, 12) -- light blue line

    -- vertical bar labels
    print("100", v_bar_x - 10, v_bar_y, 7)
    print("0", v_bar_x - 4, v_bar_y + v_bar_h - 4, 7)

    -- max performance text (this shows the same info as the new b section, but keeps the vertical bar context)
    print("max: "..tostring(current_max_perf), v_bar_x - 12, v_bar_y + v_bar_h + 4, 6)


    -- --- f. results display (coin reward only) ---
    local results_y = 110 -- shifted down

    -- only show the coin reward once the performance is locked in
    if performance_locked then
        -- print coin reward. final_coin_reward is now an integer rounded up.
        print("coin reward:", 10, results_y + 12, 7)
        print(tostring(final_coin_reward), 80, results_y + 12, 7) -- white text
    else
        print("press z to lock in!", 20, results_y, 10)
    end
end

