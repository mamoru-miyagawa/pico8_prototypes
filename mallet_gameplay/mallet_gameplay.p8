-- --- game state variables ---
local level = 1
local max_level = 50

-- timing bar state (0.0 to 1.0)
local bar_value = 0.5   -- current position of the main horizontal bar
local bar_dir = 1       -- direction of movement: 1 or -1
local bar_speed = 0.01  -- speed of bar movement per frame
local is_holding = false -- new: flag to hold the bar at 1.0
local grace_timer = 0    -- new: counter for the 0.2s grace period (6 frames at 30fps)

-- performance state
local performance_locked = false -- is a performance score currently locked in?
local target_ratio = 0.0        -- the 0.0 to 1.0 ratio locked in by z press
local target_score = 0.0        -- the final percentage fill target (target_ratio * 100)
local actual_score = 0.0        -- the score currently displayed (animates from 0 to target_score)
local achieved_points = 0.0     -- the final points acquired (target_ratio * current_max_perf)

-- reward state (calculated on pause)
local final_coin_reward = 0.0   -- final coin reward calculated

local current_max_perf = 0.0    -- max score limit for current level
local current_max_reward = 0.0  -- max reward for current level


-- load the data for the current level
function load_level_data()
    -- ensure level is within bounds
    level = mid(1, level, max_level)

    local data = level_data[level]
    current_max_perf = data.perf
    current_max_reward = data.reward
    
    -- reset points on level load
    achieved_points = 0.0
end

function _init()
    load_level_data()
end

