-- --- 1. update / logic ---
-- pico-8 convention: tab 1 handles core game logic and state updates

-- calculate the final reward and score display target
function calculate_results(locked_ratio)
    target_ratio = locked_ratio
    
    -- final points acquired (what the user sees as their score text)
    achieved_points = target_ratio * current_max_perf
    
    -- target score represents the achieved points relative 
    -- to the absolute 100-point maximum for the green bar fill.
    target_score = achieved_points
    
    -- reward calculation: coins are calculated based on the ratio and must be rounded up (ceiling).
    final_coin_reward = ceil(current_max_reward * target_ratio)
end

function _update()
    -- --- level selection (left/right) ---
    -- reset logic is integrated here
    if btnp(0) or btnp(1) then
        local prev_level = level
        if btnp(0) and level > 1 then level -= 1
        elseif btnp(1) and level < max_level then level += 1 end

        if prev_level ~= level then
            load_level_data()
            performance_locked = false 
            bar_value = 0.5
            target_ratio = 0.0
            target_score = 0.0
            actual_score = 0.0
            achieved_points = 0.0
            final_coin_reward = 0.0
            -- reset grace state on level change
            is_holding = false 
            grace_timer = 0
        end
    end

    -- --- timing bar logic (bar 1: stops when locked) ---
    if not performance_locked then
        
        -- handle holding state (grace period at 1.0)
        if is_holding then
            grace_timer -= 1
            if grace_timer <= 0 then
                is_holding = false
                bar_dir = -1 -- force reverse after grace period
            end
        else
            -- move bar only if not holding
            bar_value += bar_dir * bar_speed

            -- reverse direction if it hits the bounds (0.0 or 1.0)
            if bar_value >= 1.0 then
                bar_value = 1.0
                -- only start grace period when moving right (bar_dir == 1)
                if bar_dir == 1 then 
                    is_holding = true
                    grace_timer = 6 -- 0.2 seconds * 30 fps = 6 frames
                else
                    -- if moving left and somehow hit 1.0, just reverse immediately
                    bar_dir = -1
                end
            elseif bar_value <= 0.0 then
                bar_value = 0.0
                bar_dir = 1 -- reverse immediately at 0.0 (no grace period)
            end
        end
    end

    -- --- score bar growth logic (bar 2: animating from 0 to target) ---
    if performance_locked and actual_score < target_score then
        -- animate the score growing up to the target score for visual effect
        local growth_rate = (target_score - actual_score) * 0.1 
        if growth_rate < 0.5 then growth_rate = 0.5 end 
        
        actual_score += growth_rate
        
        -- clamp the score at the target
        if actual_score > target_score then 
            actual_score = target_score 
        end 
    end


    -- --- pause/lock-in action (z key / button 4) ---
    if btnp(4) then -- z key (pico-8 button 4)
        if not performance_locked then
            -- player performs the timing action: lock in the current bar value
            performance_locked = true
            local locked_value = bar_value -- passes the correct float value (0.0 to 1.0)
            calculate_results(locked_value)
            
        else
            -- reset for the next attempt
            performance_locked = false
            target_ratio = 0.0
            target_score = 0.0
            actual_score = 0.0
            final_coin_reward = 0.0
            achieved_points = 0.0
            bar_value = 0.5 -- start next attempt from the middle
            is_holding = false -- reset grace state
            grace_timer = 0
        end
    end

end
